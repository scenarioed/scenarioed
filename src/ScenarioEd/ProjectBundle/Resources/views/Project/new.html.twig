{% extends "::base.html.twig" %}
    {% block title %}Create a New Project{% endblock %}

    {% block body %}
        <a href="{{ path('project') }}" class="action back">&larr; Project List</a>
        <h1>{{ block('title') }}</h1>
        <form action="{{ path('project_create') }}" method="post" {{ form_enctype(form) }}>
            {{ form_widget(form) }}
                <button type="submit" class="action create">Create</button>
        </form>

      {% endblock %}
